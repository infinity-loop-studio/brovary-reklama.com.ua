<%
if (typeof page !== "undefined") {
if (page == 'контакты') {
%>
    <script>
        var map, popup, Popup;

        /** Initializes the map and the custom popup. */
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: {lat: 50.5063779, lng: 30.7767612},
                zoom: 15,
                disableDefaultUI: true
            });

            var image = '/images/logo-short.png';
            var beachMarker = new google.maps.Marker({
                position: {lat: 50.5063779, lng: 30.7767612},
                map: map,
                icon: image
            });

            var contentString = '<div id="content">' +
                '<div id="siteNotice">' +
                '</div>' +
                '<h1 id="firstHeading" class="firstHeading">Бровары Реклама</h1>' +
                '<div id="bodyContent">' +

                '<p>тел.: (097)003-40-43<br>' +
                '<div class="text-truncate font-weight-bold">Режим работы</div> <br>' +
                '<span class="text-primary h6">ПН - ПТ: 09:00 - 18:00</span><br>' +
                '<span class="text-danger h6">СБ, ВС: Выходной</span>' +
                '</div>' +
                '</div>';

            var infowindow = new google.maps.InfoWindow({
                content: contentString
            });

            infowindow.open(map, beachMarker);

            beachMarker.addListener('click', function () {
                infowindow.open(map, beachMarker);
            });

        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGWCSKUNij0405XH-qtG8ECV3jm8r_jrk&callback=initMap">
    </script>
<% }} %>
