<% if(typeof subservice !== "undefined"){ %>
    <script>
        var service = "<%= service %>"
        var language = "<%= language %>"
        var url = "/get/menu/secondary";
        var data = {
            service: service,
            language: language
        }
        io.socket.post(url, data, function (resData, menu) {
            console.log()
            var path = decodeURIComponent(window.location.pathname);
            $("#secondary-menu").append(menu.body.content);
            $("a[href$='" + path + "']").addClass("active");
        });

    </script>
<% } %>

